{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "google"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "comma-dangle": ["error", "never"],
    "indent": ["error", 2],
    "class-methods-use-this": 0,
    "max-len": [
      "error",
      {
        "code": 180
      }
    ],
    "no-new": 0,
    "no-param-reassign": 0,
    "no-new-func": 0,
    "prefer-rest-params": 0,
    "require-jsdoc": 0
  }
}
